echo on
setenv DEBUG "FALSE"
setenv BN_FILENAME "./bn/ex/Alarm_JB.bif"
setenv PRUNE_PREC "-1"
setenv PRUNE_TYPE "none"
// DD_TYPE can be TABLE, ADD, AADD
setenv DD_TYPE "AADD"
timer start
load
timer stop

// Display Bayes net DAG and print BN statistics
viz
print
viz BP
print BP

// Query #1
setenv BP "query"
setenv TPR "query"
setenv ERRLOWOUTPUT "assign:TRUE"
setenv PVSAT "assign:HIGH"
setenv LVFAILURE "assign:TRUE"
timer start
query
timer stop

quit
